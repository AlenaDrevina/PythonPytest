pipeline {
    agent any
    stages {
    
    stage('SCM checkout'){
    echo"pulling changes from the branch ${params.branch}"
    git url:'https://github.com/AlenaDrevina/PythonPytest', branch: 'Module5'
     }
     
      stage('Build') {
        steps {
          sh 'python3 t1.py'
        }
      }
      stage('Test') {
        steps {
          sh 'python3 -m pytest -v -s t1.py --html=report.html'
        }
      }
    }    
  }

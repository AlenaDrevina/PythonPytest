pipeline {
    agent any
    stages {
   
      stage('Build') {
        steps {
          git branch: 'main', url: 'https://github.com/AlenaDrevina/PythonPytest.git'
        }
      }
      stage('Test') {
        steps {
          sh 'python3 t1.py'
        }
      }
      stage('Deploy') {
        steps {
         sh 'git checkout pre-prod'
         sh 'def now = new Date()'
         sh 'git commit -m now.format("yyMMdd.HHmm")'
         sh 'git merge main'
        }
      }
      stage('Print') {
     steps {
       echo 'Well done!'
     }
   }
    }    
  }
